{% if section.settings.enable_timeline and timeline_columns.size > 0 %}
<script>
	(function () {
		const sectionId = '{{ section.id }}';
		const sectionEl = document.querySelector(`#shopify-section-${sectionId}`);
		if (!sectionEl) return;

		if (sectionEl.__iwtCleanup) sectionEl.__iwtCleanup();

		const list = sectionEl.querySelector('[data-iwt-list]');
		const steps = Array.from(sectionEl.querySelectorAll('.iwt-step'));
		const dots = steps.map((s) => s.querySelector('.iwt-step__dot')).filter(Boolean);
		const markers = steps.map((s) => s.querySelector('.iwt-step__marker'));
		const contents = steps.map((s) => s.querySelector('.iwt-step__content')).filter(Boolean);
		const titles = steps.map((s) => s.querySelector('[data-iwt-title]'));

		if (!list || steps.length === 0 || dots.length === 0) return;

		const thresholdRatio = 0.45;
		const fadeRange = {{ section.settings.timeline_fade_range }};

		const inactiveColorVar = getComputedStyle(list).getPropertyValue('--iwt-inactive-color').trim() || '{{ section.settings.timeline_inactive_color }}';
		const activeColorVar = getComputedStyle(list).getPropertyValue('--iwt-active-color').trim() || '{{ section.settings.timeline_active_color }}';

		const clamp01 = (n) => Math.max(0, Math.min(1, n));
		const lerp = (a, b, t) => a + (b - a) * t;
		const lerpColor = (a, b, t) => `rgb(${Math.round(lerp(a[0], b[0], t))} ${Math.round(lerp(a[1], b[1], t))} ${Math.round(lerp(a[2], b[2], t))})`;

		function toRgbTriplet(colorValue) {
			const tmp = document.createElement('span');
			tmp.style.color = colorValue;
			tmp.style.display = 'none';
			document.body.appendChild(tmp);
			const computed = getComputedStyle(tmp).color;
			tmp.remove();
			const match = computed.match(/rgba?\((\d+),\s*(\d+),\s*(\d+)/i);
			if (!match) return [17, 17, 17];
			return [parseInt(match[1], 10), parseInt(match[2], 10), parseInt(match[3], 10)];
		}

		const inactiveRgb = toRgbTriplet(inactiveColorVar);
		const activeRgb = toRgbTriplet(activeColorVar);

		let lineTop = 0;
		let lineHeight = 1;
		let hasLayout = false;

		function layoutDotsAndLine() {
			const listRect = list.getBoundingClientRect();
			const listTop = listRect.top;
			const positions = [];

			steps.forEach((step, i) => {
				const dot = dots[i];
				const title = titles[i];
				const marker = markers[i];
				if (!dot || !title || !marker) return;

				const markerRect = marker.getBoundingClientRect();
				const titleRect = title.getBoundingClientRect();
				const dotY = (titleRect.top + titleRect.height / 2) - markerRect.top;
				dot.style.top = `${dotY}px`;

				const dotRect = dot.getBoundingClientRect();
				positions.push(dotRect.top + dotRect.height / 2 - listTop);
			});

			if (positions.length === 0) return;
			const first = positions[0];
			const last = positions[positions.length - 1];
			lineTop = first;
			lineHeight = Math.max(1, last - first);
			list.style.setProperty('--iwt-line-top', `${lineTop}px`);
			list.style.setProperty('--iwt-line-height', `${lineHeight}px`);

			const marker0 = markers[0] ? markers[0].getBoundingClientRect() : dots[0].getBoundingClientRect();
			list.style.setProperty('--iwt-line-x', `${marker0.left + marker0.width / 2 - listRect.left}px`);
			hasLayout = true;
		}

		let raf = null;
		function update() {
			if (!hasLayout) layoutDotsAndLine();

			const listRect = list.getBoundingClientRect();
			const listTop = listRect.top;
			const listHeightPx = listRect.height || 1;
			const thresholdY = window.innerHeight * thresholdRatio;

			const progressPx = Math.max(0, Math.min(listHeightPx, thresholdY - listTop));
			const normalized = Math.max(0, Math.min(lineHeight, progressPx - lineTop));
			const progressRatio = clamp01(normalized / lineHeight);
			list.style.setProperty('--iwt-progress', String(progressRatio));

			dots.forEach((dot) => {
				const rect = dot.getBoundingClientRect();
				const centerY = rect.top + rect.height / 2;
				const passed = centerY <= thresholdY;
				const dist = Math.max(0, centerY - thresholdY);
				const t = passed ? 1 : clamp01(1 - dist / fadeRange);
				dot.style.backgroundColor = lerpColor(inactiveRgb, activeRgb, t);
			});

			contents.forEach((content, i) => {
				const dot = dots[i];
				if (!dot) return;
				const rect = dot.getBoundingClientRect();
				const centerY = rect.top + rect.height / 2;
				const passed = centerY <= thresholdY;
				const dist = Math.max(0, centerY - thresholdY);
				const t = passed ? 1 : clamp01(1 - dist / fadeRange);
				content.style.color = lerpColor(inactiveRgb, activeRgb, t);
			});
		}

		function onScrollOrResize() {
			if (raf) return;
			raf = requestAnimationFrame(() => {
				raf = null;
				update();
			});
		}

		function onResize() {
			hasLayout = false;
			onScrollOrResize();
		}

		window.addEventListener('scroll', onScrollOrResize, { passive: true });
		window.addEventListener('resize', onResize);

		sectionEl.__iwtCleanup = () => {
			window.removeEventListener('scroll', onScrollOrResize);
			window.removeEventListener('resize', onResize);
			if (raf) cancelAnimationFrame(raf);
			raf = null;

			list.style.removeProperty('--iwt-progress');
			list.style.removeProperty('--iwt-line-x');
			list.style.removeProperty('--iwt-line-top');
			list.style.removeProperty('--iwt-line-height');

			dots.forEach((dot) => {
				dot.style.backgroundColor = '';
				dot.style.top = '';
			});

			contents.forEach((c) => {
				c.style.color = '';
			});
		};

		layoutDotsAndLine();
		update();

		if (window.Shopify && Shopify.designMode) {
			document.addEventListener('shopify:section:unload', (event) => {
				if (!event || !event.detail || event.detail.sectionId !== sectionId) return;
				if (sectionEl.__iwtCleanup) sectionEl.__iwtCleanup();
			});
		}
	})();
</script>
{% endif %}

