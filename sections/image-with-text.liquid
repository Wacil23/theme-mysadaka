<div class='image-with-text color-{{ section.settings.color_scheme }} background section-{{ section.id }}-padding'>
	<div class='container'>
		{% render 'section-text',
			section: section,
			class: 'image-with-text',
			text_align: section.settings.text_align,
			heading_level: section.settings.heading_level
		%}

		<div class='image-with-text__content content'>
			<div class='image-with-text__grid image-with-text__grid--{{ section.settings.layout }} {% if section.settings.layout == 'text_last' %} image-with-text__grid--reverse{% endif %} image-with-text__grid--{{ section.settings.text_align_inner }}'>
				{% if section.settings.layout == 'image_center' %}
					{% if section.blocks.size > 0 %}
						<div class='image-with-text__columns image-with-text__columns--image_center'>
							{% for block in section.blocks %}
								{% assign mod = forloop.index | modulo: 2 %}
								{% if mod == 1 %}
									{% if section.settings.enable_timeline %}
										{% continue %}
									{% endif %}
									{% unless block.type == 'column' %}
										{% continue %}
									{% endunless %}
									<div
										class='image-with-text__column'
										{{ block.shopify_attributes }}
									>
										{% if block.settings.icon != 'none' %}
											<div
												class='
													image-with-text__column-icon
													image-with-text__column-icon--{{- section.settings.icon_size }}
													{% if section.settings.box_shadow %}image-with-text__column-icon--shadow {% endif %}
													image-with-text__column-icon--{{- section.settings.icon_style }}
													image-with-text__column-icon--{{- block.settings.icon_fill_type }}
												'
											>
												<i
													class='icon icon-pack icon-{{ block.settings.icon }} {% if section.settings.icon_style == 'colored' and block.settings.icon_fill_type == 'gradient' %} text-gradient {% endif %}'
												></i>
											</div>
										{% endif %}
										{% if block.settings.heading != blank %}
											<div class='image-with-text__column-title h5'>
												{{ block.settings.heading }}
											</div>
										{% endif %}
										{% if block.settings.text %}
											<div class='image-with-text__column-text'>
												{{ block.settings.text }}
											</div>
										{% endif %}
									</div>
								{% endif %}
							{% endfor %}
						</div>
					{% endif %}
				{% endif %}
				<div class='image-with-text__left image-with-text__left--{{ section.settings.height }}'>
					{% if section.settings.enable_link == true and section.settings.image != blank %}<a {% if section.settings.link != blank %} href="{{section.settings.link}}"{% endif %} {% if section.settings.link != blank %} target="blank"{% endif %}>{% endif %}
					<div
						class='
							image-with-text__media
							image-with-text__media--{{ section.settings.height }}
							{% if section.settings.height == 'all-height' and section.settings.layout == 'image_center' %}
							  {% if section.settings.inner_heading != blank or section.settings.description != blank
							    or section.settings.show_button or section.settings.show_button_2 %}
							    image-with-text__media--all-height-adapt
							  {% endif %}
							{% endif %}
							{% if section.settings.image != blank %}
							{% else %}image-with-text__media--placeholder placeholder{% endif %}
						'
					>
						<div class='image-with-text__top color-{{ section.settings.author_color_scheme }}'>
							{% if section.settings.logo != blank
								or section.settings.logo_dm
							%}
								{%- assign width = section.settings.logo_width -%}
								{%- assign width_adapt = section.settings.logo_width
									| times: 0.8
									| round: 2
								-%}

								{% capture sizes %}
                  (min-width: 1100px) {{ width }}px, 
                  (min-width: 750px) {{ width_adapt }}px
                {% endcapture %}

								{% capture widths %}
                  {{ width | times: 2 }}, {{ width_adapt | times: 2 }}
                {% endcapture %}

								{% capture style %}
                  --width-adapt: {{ width_adapt }}px
                {% endcapture %}

								<div class='image-with-text__logos'>
									{%- if section.settings.logo_dm != blank -%}
										{{
											section.settings.logo_dm
											| image_url: width: width
											| image_tag:
												loading: 'lazy',
												sizes: sizes,
												widths: widths,
												height: 'auto',
												width: width,
												class: ' image-with-text__logo image-with-text__logo--dark',
												style: style
										}}
									{%- endif -%}
									{%- if section.settings.logo != blank -%}
										{{
											section.settings.logo
											| image_url: width: width
											| image_tag:
												loading: 'lazy',
												sizes: sizes,
												widths: widths,
												height: 'auto',
												width: width,
												class: ' image-with-text__logo image-with-text__logo--light',
												style: style
										}}
									{%- endif -%}
								</div>
							{% endif %}
							{% if section.settings.add_text %}
								<div class='image-with-text__add-text subtitle'>
									{{ section.settings.add_text }}
								</div>
							{% endif %}
						</div>

						{%- if section.settings.image != blank -%}
							{% capture sizes %}
                (min-width: 990px) 50vw - 40px, 
                calc((100vw - 40px))
              {% endcapture %}
							{{
								section.settings.image
								| image_url: width: section.settings.image.width
								| image_tag:
									loading: 'lazy',
									sizes: sizes,
									widths: '360, 535, 720, 940, 1070, 1200, 1400, 1600, 1800, 2000, 2200'
							}}
						{%- else -%}
							{{ 'hero-apparel-3' | placeholder_svg_tag: 'placeholder-svg' }}
						{%- endif -%}
					</div>
				{% if section.settings.enable_link == true and section.settings.image != blank %}</a>{% endif %}
				</div>
				{% if section.settings.layout == 'image_center' %}
					{% if section.blocks.size > 0 %}
						<div class='image-with-text__columns image-with-text__columns--image_center'>
							{% for block in section.blocks %}
								{% assign mod = forloop.index | modulo: 2 %}
								{% if mod == 0 %}
									{% if section.settings.enable_timeline %}
										{% continue %}
									{% endif %}
									<div
										class='image-with-text__column'
										{{ block.shopify_attributes }}
									>
										{% if block.settings.icon != 'none' %}
											<div
												class='
													image-with-text__column-icon
													image-with-text__column-icon--{{- section.settings.icon_size }}
													{% if section.settings.box_shadow %}image-with-text__column-icon--shadow {% endif %}
													image-with-text__column-icon--{{- section.settings.icon_style }}
													image-with-text__column-icon--{{- block.settings.icon_fill_type }}
												'
											>
												<i
													class='icon icon-pack icon-{{ block.settings.icon }} {% if section.settings.icon_style == 'colored' and block.settings.icon_fill_type == 'gradient' %} text-gradient {% endif %}'
												></i>
											</div>
										{% endif %}
										{% if block.settings.heading != blank %}
											<div class='image-with-text__column-title h5'>
												{{ block.settings.heading }}
											</div>
										{% endif %}
										{% if block.settings.text %}
											<div class='image-with-text__column-text'>
												{{ block.settings.text }}
											</div>
										{% endif %}
									</div>
								{% endif %}
							{% endfor %}
						</div>
					{% endif %}
				{% endif %}
				{% if section.settings.layout != 'image_center' %}
					<div class='image-with-text__right'>
						{%- if section.settings.inner_subheading != blank -%}
							<div class='image-with-text__inner-subheading subtitle'>
								{{ section.settings.inner_subheading }}
							</div>
						{%- endif -%}
						{%- if section.settings.inner_heading != blank -%}
							<{{ section.settings.inner_heading_level }}
								class='
									image-with-text__inner-heading title--section title--{{ section.settings.inner_heading_line }} {{ section.settings.inner_heading_size }}
									{% if section.settings.inner_word_break and section.settings.inner_heading_line != 'none' %} title--word_break {% endif %}
								'
								style='--color-line-inner: {{ section.settings.inner_line_color }}'
							>
								{{ section.settings.inner_heading }}
							</{{ section.settings.inner_heading_level }}>
						{%- endif -%}
						{% if section.settings.description != blank %}
							<div class='image-with-text__text body-large'>
								{{ section.settings.description }}
							</div>
						{% endif %}

						{% if section.blocks.size > 0 and section.settings.enable_timeline == false %}
							<div class='image-with-text__columns'>
								{% for block in section.blocks %}
									<div
										class='image-with-text__column'
										{{ block.shopify_attributes }}
									>
										{% unless block.type == 'column' %}
											{% continue %}
										{% endunless %}
										{% if block.settings.icon != 'none' %}
											<div
												class='
													image-with-text__column-icon
													image-with-text__column-icon--{{- section.settings.icon_size }}
													{% if section.settings.box_shadow %}image-with-text__column-icon--shadow {% endif %}
													image-with-text__column-icon--{{- section.settings.icon_style }}
													image-with-text__column-icon--{{- block.settings.icon_fill_type }}
												'
											>
												<i
													class='icon icon-pack icon-{{ block.settings.icon }} {% if section.settings.icon_style == 'colored' and block.settings.icon_fill_type == 'gradient' %} text-gradient {% endif %}'
												></i>
											</div>
										{% endif %}
										{% if block.settings.heading != blank %}
											<div class='image-with-text__column-title h5'>
												{{ block.settings.heading }}
											</div>
										{% endif %}
										{% if block.settings.text %}
											<div class='image-with-text__column-text'>
												{{ block.settings.text }}
											</div>
										{% endif %}
									</div>
								{% endfor %}
							</div>
						{% endif %}

						{% if section.settings.enable_timeline and timeline_columns.size > 0 %}
							<div class="iwt-timeline" data-iwt-timeline>
								<ul class="iwt-timeline__list" data-iwt-list>
									{% for block in timeline_columns %}
										<li class="iwt-step" {{ block.shopify_attributes }}>
