{{ 'section-image-with-text.css' | asset_url | stylesheet_tag }}

{%- style -%}
	{% render 'section-padding' %}
	{% render 'section-margin' %}

	.section-{{ section.id }}-padding .image-with-text__media::before {
	  background-color: rgba(var(--color-overlay), {{ section.settings.overlay_opacity }});
	}

	{% if section.settings.image != blank %}
	  .section-{{ section.id }}-padding .image-with-text__media--adapt,
	  .section-{{ section.id }}-padding .image-with-text__media--all-height-adapt {
	    padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 | round: 2 }}%;
	  }
	{% endif %}
{%- endstyle -%}

{%- liquid
	assign timeline_columns = section.blocks | where: 'type', 'column' | slice: 0, 5
-%}

{% style %}
	#shopify-section-{{ section.id }} .iwt-timeline {
		margin-top: 2.4rem;
	}

	#shopify-section-{{ section.id }} .iwt-timeline__list {
		list-style: none;
		margin: 0;
		padding: 0;
		position: relative;
		display: flex;
		flex-direction: column;
		gap: {{ section.settings.timeline_steps_gap }}px;
		--iwt-progress: 0;
		--iwt-line-x: 8px;
		--iwt-line-top: 0px;
		--iwt-line-height: 100%;
		--iwt-inactive-color: {{ section.settings.timeline_inactive_color }};
		--iwt-active-color: {{ section.settings.timeline_active_color }};
		isolation: isolate;
	}

	#shopify-section-{{ section.id }} .iwt-timeline__list::before,
	#shopify-section-{{ section.id }} .iwt-timeline__list::after {
		content: '';
		position: absolute;
		left: var(--iwt-line-x);
		top: var(--iwt-line-top);
		height: var(--iwt-line-height);
		width: {{ section.settings.timeline_line_width }}px;
		border-radius: 999px;
		transform: translateX(-50%);
		z-index: 0;
	}

	#shopify-section-{{ section.id }} .iwt-timeline__list::before {
		background: var(--iwt-inactive-color);
		opacity: 1;
	}

	#shopify-section-{{ section.id }} .iwt-timeline__list::after {
		background: var(--iwt-active-color);
		transform-origin: top;
		transform: translateX(-50%) scaleY(var(--iwt-progress));
		will-change: transform;
		z-index: 1;
	}

	#shopify-section-{{ section.id }} .iwt-step {
		position: relative;
		display: grid;
		grid-template-columns: calc({{ section.settings.timeline_dot_size }}px + 6px) minmax(0, 1fr);
		column-gap: {{ section.settings.timeline_marker_gap }}px;
		align-items: start;
		z-index: 2;
	}

	#shopify-section-{{ section.id }} .iwt-step__marker {
		position: relative;
		min-height: 1px;
	}

	#shopify-section-{{ section.id }} .iwt-step__dot {
		position: absolute;
		left: 50%;
		top: 0;
		width: {{ section.settings.timeline_dot_size }}px;
		height: {{ section.settings.timeline_dot_size }}px;
		border-radius: 999px;
		background: var(--iwt-inactive-color);
		transform: translate(-50%, -50%);
		will-change: background-color, transform;
		z-index: 3;
	}

	#shopify-section-{{ section.id }} .iwt-step__content {
		color: var(--iwt-inactive-color);
		will-change: color;
		position: relative;
		z-index: 2;
	}

	#shopify-section-{{ section.id }} .iwt-step__title {
		font-weight: 700;
		text-transform: none;
	}

	#shopify-section-{{ section.id }} .iwt-step__text {
		margin-top: {{ section.settings.timeline_title_text_gap }}px;
	}
{% endstyle %}

